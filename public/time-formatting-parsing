      onkeydown = (e) => {
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'epoch';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'random-numbers';
          }
          
      }

      function onChange(e) {
        window.location.href = event.target.value
      }

