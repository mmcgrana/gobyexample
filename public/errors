      onkeydown = (e) => {
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'interfaces';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'goroutines';
          }
          
      }

      function onChange(e) {
        window.location.href = event.target.value
      }

