      onkeydown = (e) => {
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'regular-expressions';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'xml';
          }
          
      }

      function onChange(e) {
        window.location.href = event.target.value
      }

